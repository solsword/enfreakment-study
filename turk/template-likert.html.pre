<script type="text/javascript">
// Interaction setup
$(document).ready(function() {
  $(".responses input").on("click", function(event) {
    var cell = $(this).parent();
    var row = cell.parent();
    row.addClass("touched");
    row.children('td').each(function (i, el) {
      $(el).removeClass("selected");
    });
    cell.addClass("selected");
  });
  $(".question_answers .answer input").on("click", function(event) {
    var ans = $(this).parent();
    var area = ans.parent();
    area.addClass("touched");
    area.children(".answer").each(function (i, el) {
      $(el).removeClass("selected");
    });
    ans.addClass("selected");
  });
  $("div .associated_text").on("click keypress", function(event) {
    var group = $(this).parent();
    group.children(".associated_radio").prop("checked", true);
  });
  $("td .associated_text").on("click keypress", function(event) {
    var row = $(this).parent().parent();
    row.children("td").each(function(i, cell) {
      $(cell).children(".associated_radio").prop("checked", true);
    });
  });
  var tq_inputs = $(".text_question input[type=text]");
  tq_inputs = tq_inputs.add($(".text_question textarea"));
  console.log(tq_inputs);
  tq_inputs.on("click keypress", function(event) {
    var here = $(this);
    while(!here.hasClass("text_question")) {
      here = here.parent();
    }
    here.addClass("touched");
  });
  $("#ethnicity_questions input").on("click keypress", function(event) {
    var cell = $(this).parent();
    var row = cell.parent();
    var table = row.parent().parent();
    var bin = table.parent();
    bin.addClass("touched");
    var checkbox = row.find("input:checked");
    if (checkbox.length > 0) {
      row.addClass("selected");
    } else {
      row.removeClass("selected");
    }
  });
});
</script>
